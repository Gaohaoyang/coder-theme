{
    "manifest_version": 2,

    "name": "coder theme",
    "description": "This extension shows a Google Image search result for the current page",
    "version": "1.0",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html",
        "default_title": "Change Theme!"
    },
    "permissions": [
        "https://www.zhihu.com/*"
    ],
    "content_scripts": [{
        "matches": ["https://www.zhihu.com/*"],
        "css": ["zhihu.css"]
    }]
}
